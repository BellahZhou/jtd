<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jtd.dao.LyDao">
	<resultMap id="BaseResultMap" type="com.jtd.domain.Ly">
        <id column="ID" property="id" jdbcType="DECIMAL"/>
        <result column="USER_ID" property="userId" jdbcType="DECIMAL"/>
        <result column="REMARK" property="remark" jdbcType="VARCHAR"/>
        <result column="REBACK" property="reback" jdbcType="VARCHAR"/>
        <result column="CREATE_BY" property="createBy" jdbcType="VARCHAR"/>
        <result column="CREATE_DATE" property="createDate" jdbcType="TIMESTAMP"/>
        <result column="UPDATE_BY" property="updateBy" jdbcType="VARCHAR"/>
        <result column="UPDATE_DATE" property="updateDate" jdbcType="TIMESTAMP"/>
    </resultMap>

    <select id="getLysByUserId" parameterType="Long" resultMap="BaseResultMap">
        SELECT * FROM tb_ly WHERE user_id=#{userId}
    </select>
    
    <insert  id="insert">
   		insert  into `tb_ly`(`USER_ID`,`REMARK`,`CREATE_BY`,`CREATE_DATE`,`UPDATE_BY`,`UPDATE_DATE`) values(#{userId},#{remark},#{createBy},#{createDate},#{updateBy},#{updateDate})
  	</insert>
</mapper>