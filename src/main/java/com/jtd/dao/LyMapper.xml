<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jtd.dao.LyDao">


    <select id="getLysByUserId" parameterType="Long" resultType="Ly">
        SELECT * FROM tb_ly WHERE user_id=#{userId}
    </select>
    
    <insert  id="insert">
   		insert  into `tb_ly`(`USER_ID`,`REMARK`,`CREATE_BY`,`CREATE_DATE`,`UPDATE_BY`,`UPDATE_DATE`) values(#{userId},#{remark},#{createBy},#{createDate},#{updateBy},#{updateDate})
  	</insert>
</mapper>